{
  "name": "@enterprise-cmcs/macpro-serverless-running-stages",
  "version": "1.0.0",
  "description": "Common UX libraries packaged for easier distribution throughout CMS.",
  "main": "build/index.js",
  "typings": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "test": "npm run gen && node tests/run.mjs",
    "bp": "rm -rf ./dist && rm -rf ./node_modules && npm i --save && tsc && npm publish",
    "gen": "tsc",
    "test2": "jest",
    "coverage": "jest --coverage",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=10.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.128.0",
    "@types/readline-sync": "^1.4.4",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.0.4",
    "lodash": "^4.17.21",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "gulp": "^4.0.2",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.0.1",
    "sass": "^1.53.0",
    "sass-loader": "10.1.1",
    "semantic-release": "^19.0.3",
    "tslog": "^4.4.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/Enterprise-CMCS/macpro-serverless-running-stages/issues"
  },
  "keywords": [
    "Serverless Running Stages",
    "Serverless",
    "Running",
    "Stages",
    "Environments"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Enterprise-CMCS/macpro-serverless-running-stages"
  },
  "license": "CC0-1.0",
  "author": {
    "name": "Center for Medicare & Medicaid Services",
    "url": "https://www.cms.gov/"
  },
  "homepage": "https://github.com/Enterprise-CMCS/macpro-serverless-running-stages#readme"
}
